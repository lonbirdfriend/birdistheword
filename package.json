{
  "name": "bird-learning-app",
  "version": "1.0.0",
  "description": "Interactive bird learning application with spaced repetition system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "node scripts/setup.js",
    "reset-db": "node scripts/reset-database.js"
  },
  "keywords": [
    "birds",
    "learning",
    "memory-game",
    "spaced-repetition",
    "nature",
    "education",
    "nodejs",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "bcryptjs": "^2.4.3",
    "sqlite3": "^5.1.6",
    "ejs": "^3.1.9",
    "node-fetch": "^2.6.12",
    "multer": "^1.4.5-lts.1",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.53.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/bird-learning-app.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/bird-learning-app/issues"
  },
  "homepage": "https://github.com/yourusername/bird-learning-app#readme",
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "*.db"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn"
    }
  }
}
